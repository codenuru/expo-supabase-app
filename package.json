    "@react-navigation/stack": "^6.0.11",
    "@reduxjs/toolkit": "^1.7.2",
    "@supabase/supabase-js": "^1.30.0",
    "axios": "^0.25.0",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "expo": "~44.0.6",
    "expo-dev-client": "^0.8.4",
    "expo-image-picker": "~12.0.1",
